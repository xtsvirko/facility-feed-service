{
  "Name": "facility-feed-scheduler",
  "ScheduleExpression": "rate(1 day)",
  "State": "ENABLED",
  "Targets": [
    {
      "Id": "facility-feed-task",
      "Arn": "arn:aws:ecs:${AWS_REGION}:${AWS_ACCOUNT_ID}:cluster/${ECS_CLUSTER_NAME}",
      "RoleArn": "arn:aws:iam::${AWS_ACCOUNT_ID}:role/ecsEventsRole"
    }
  ]
}